int Solution::diffPossible(vector<int> &A, int B) {
    int i=0;
    int j=1;
    while(i<A.size() && j<A.size())
    {
        if(A[j]-A[i]==B && j>i)
        {
            return 1;
        }
        if(A[j]-A[i]>B && j>i)
        {
            i++;
        }
        else
        {
            j++;
        }
    }
    return 0;
}
