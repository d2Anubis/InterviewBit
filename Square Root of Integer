int Solution::sqrt(int A) {
    int num=A;
    if(num==0||num==1)
    {
        return num;
    }
    int st=1;
    int fnsh=num/2;
    int mid;
    while(st<fnsh)
    {
        mid=(st+fnsh+1)/2;
        if((mid*mid)>A)
        {
            fnsh=mid-1;
        }
        if((mid*mid)<A)
        {
           st=mid;
        }
    }
    return st;
}
